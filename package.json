{
  "name": "lambda-svelte-skeleton",
  "version": "0.0.1",
  "description": "Template for Lambda, S3 Full-stack example with Svelte.",
  "private": true,
  "scripts": {
    "build": "concurrently -p \"[{name} / {time}]\" -c \"green,magenta\" -n \"client,server\" \"pnpm run -r --filter=client build \" \"pnpm run -r -filter=server build\"",
    "dev": "concurrently -p \"[{name} / {time}]\" -c \"green,magenta\" -n \"client,server\" \"pnpm run -r --filter=client dev \" \"pnpm run -r -filter=server dev\"",
    "delete": "pnpm run --filter=client delete && pnpm run --filter=server delete",
    "deploy": "node ./scripts/deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EATSTEAK/lambda-svelte-skeleton.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "author": "EATSTEAK <me@eatsteak.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EATSTEAK/lambda-svelte-skeleton/issues"
  },
  "homepage": "https://github.com/EATSTEAK/lambda-svelte-skeleton#readme"
}
