{
	"name": "client",
	"version": "0.0.1",
	"scripts": {
		"dev": "cross-env VITE_BASE_URL=http://localhost:3000 vite dev --port 5002 --host 0.0.0.0",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"prettier": "prettier --check --plugin-search-dir=. . ",
		"lint": "eslint .",
		"format": "eslint . --fix && prettier --write --plugin-search-dir=. .",
		"delete": "node ./scripts/delete.js",
		"deploy": "node ./scripts/deploy.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.24",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.5.0",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.1",
		"sass": "^1.46.0",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.11",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"type": "module",
	"dependencies": {
		"vite": "^3.1.3"
	}
}
